                    <TextView
                        android:text=" 副専攻"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:textSize="18sp"
                        android:textColor="@color/Black"
                        android:id="@+id/userinfo8"
                        android:layout_weight="5"
                        />
